{
  "name": "react-free-style",
  "version": "0.3.4",
  "description": "Make React components easier and more maintainable by using inline style objects with Free Style",
  "main": "dist/react-free-style.js",
  "files": [
    "dist/",
    "typings/",
    "react-free-style.d.ts",
    "LICENSE"
  ],
  "scripts": {
    "lint": "# TODO",
    "build": "npm run build-ts",
    "build-ts": "tsc src/*.ts --declaration --sourceMap --removeComments --noImplicitAny --outDir dist --target es5 --module commonjs",
    "test-spec": "mocha -R spec --bail",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec --bail",
    "test": "npm run build && npm run lint && npm run test-cov",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/react-free-style.git"
  },
  "typescript": {
    "definition": "react-free-style.d.ts"
  },
  "keywords": [
    "style",
    "css",
    "object",
    "react",
    "inline"
  ],
  "author": "Blake Embrey <hello@blakeembrey.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blakeembrey/react-free-style/issues"
  },
  "homepage": "https://github.com/blakeembrey/react-free-style",
  "devDependencies": {
    "chai": "^2.1.1",
    "istanbul": "^0.3.5",
    "mocha": "^2.1.0",
    "pre-commit": "^1.0.6",
    "standard": "^2.1.1",
    "typescript": "^1.4.1"
  },
  "dependencies": {
    "free-style": "^0.5.2"
  },
  "peerDependencies": {
    "react": ">=0.12.0"
  }
}
