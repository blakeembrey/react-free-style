{
  "name": "react-free-style",
  "version": "8.0.0",
  "description": "Make React components easier and more maintainable by using JavaScript with Free Style",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prettier": "prettier --write",
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "format": "npm run prettier -- README.md \"{src,example}/**/*.{js,ts}\"",
    "build": "rimraf dist && tsc",
    "specs": "jest --coverage",
    "test": "npm run -s lint && npm run -s build && npm run -s specs",
    "prepare": "npm run build",
    "example": "cd example && webpack-dev-server --config webpack.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/react-free-style.git"
  },
  "keywords": [
    "style",
    "css",
    "object",
    "react",
    "inline"
  ],
  "author": "Blake Embrey <hello@blakeembrey.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blakeembrey/react-free-style/issues"
  },
  "homepage": "https://github.com/blakeembrey/react-free-style",
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "npm run prettier",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/react": "^16.7.5",
    "@types/react-dom": "^16.0.9",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "jest": "^24.0.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.2",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "rimraf": "^2.6.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.4",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.8"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "free-style": "^2.5.2",
    "style-helper": "^3.0.0"
  }
}
