{
  "name": "react-free-style",
  "version": "1.1.0",
  "description": "Make React components easier and more maintainable by using inline style objects with Free Style",
  "main": "dist/react-free-style.js",
  "files": [
    "dist/",
    "typings.json",
    "LICENSE"
  ],
  "scripts": {
    "lint": "# TODO",
    "build": "rm -rf dist/ && tsc",
    "test-spec": "mocha dist/**/*.spec.js -R spec --bail",
    "test-cov": "istanbul cover -x dist/**/*.spec.js node_modules/mocha/bin/_mocha -- dist/**/*.spec.js -R spec --bail",
    "test": "npm run build && npm run lint && npm run test-cov",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/react-free-style.git"
  },
  "keywords": [
    "style",
    "css",
    "object",
    "react",
    "inline"
  ],
  "author": "Blake Embrey <hello@blakeembrey.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blakeembrey/react-free-style/issues"
  },
  "homepage": "https://github.com/blakeembrey/react-free-style",
  "devDependencies": {
    "chai": "^3.4.1",
    "istanbul": "^0.4.1",
    "mocha": "^2.1.0",
    "ntypescript": "^1.201508100803.1",
    "pre-commit": "^1.0.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "typescript": "^1.7.5",
    "typings": "^0.3.5"
  },
  "dependencies": {
    "free-style": "^1.0.0",
    "xtend": "^4.0.0"
  }
}
